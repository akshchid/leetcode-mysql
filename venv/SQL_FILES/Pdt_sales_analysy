-- Create Product table
CREATE TABLE Product1 (
    product_id INT PRIMARY KEY,
    product_name VARCHAR(100)
);

-- Create Sales table
CREATE TABLE Sales1 (
    sale_id INT PRIMARY KEY,
    product_id INT,
    year INT,
    quantity INT,
    price INT,
    FOREIGN KEY (product_id) REFERENCES Product(product_id)
);
-- Insert data into Product table
INSERT INTO Product1 (product_id, product_name) VALUES
(100, 'Nokia'),
(200, 'Apple'),
(300, 'Samsung');

-- Insert data into Sales table
INSERT INTO Sales1 (sale_id, product_id, year, quantity, price) VALUES
(1, 100, 2008, 10, 5000),
(2, 100, 2009, 12, 5000),
(7, 200, 2011, 15, 9000);

SELECT 
    product_id, SUM(quantity) AS total_quantity
FROM
    Sales
GROUP BY product_id;

